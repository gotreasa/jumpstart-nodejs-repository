{
  "name": "jumpstart-nodejs-repository",
  "version": "1.0.0",
  "description": "Jumpstart project for starting a NodeJS repository",
  "scripts": {
    "detect-secrets": "detect-secrets-launcher * --baseline .secrets-baseline",
    "lint": "eslint --cache --fix",
    "precommit": "npm-run-all lint",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gotreasa/jumpstart-nodejs-repository.git"
  },
  "author": "Gearoid O'Treasaigh",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/gotreasa/jumpstart-nodejs-repository/issues"
  },
  "homepage": "https://github.com/gotreasa/jumpstart-nodejs-repository#readme",
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@types/eslint": "^7.2.10",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/prettier": "^2.2.3",
    "detect-secrets": "^1.0.6",
    "eslint": "^7.25.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "snyk": "^1.566.0"
  },
  "engines": {
    "node": "v14.17.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm-run-all detect-secrets lint"
    }
  }
}
